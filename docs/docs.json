{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "StashFyle",
  "colors": {
    "primary": "#344966",
    "light": "#4a6a8a",
    "dark": "#344966"
  },
  "favicon": "/images/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "errors",
              "rate-limits",
              "plans"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api-reference/overview",
              "api-reference/upload",
              "api-reference/files",
              "api-reference/signed-urls",
              "api-reference/usage"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/elementnl/StashFyle",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/images/logo-light.svg",
    "dark": "/images/logo-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://stashfyle.com/dashboard"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://stashfyle.com/api-keys"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/elementnl/StashFyle"
    }
  },
  "api": {
    "baseUrl": "https://api.stashfyle.com",
    "auth": {
      "method": "bearer"
    }
  }
}
