{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "StashFyle",
  "appearance": {
    "default": "light"
  },
  "colors": {
    "primary": "#344966",
    "light": "#4a6a8a",
    "dark": "#344966"
  },
  "favicon": "/images/favicon.svg",
  "sidebar": {
    "user": {
      "visibility": "hidden"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "errors",
              "rate-limits",
              "plans"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "book-open",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/overview"
            ]
          },
          {
            "group": "Uploads",
            "pages": [
              "api-reference/upload"
            ]
          },
          {
            "group": "Files",
            "pages": [
              "api-reference/list-files",
              "api-reference/get-file",
              "api-reference/delete-file"
            ]
          },
          {
            "group": "Signed URLs",
            "pages": [
              "api-reference/signed-urls"
            ]
          },
          {
            "group": "Account",
            "pages": [
              "api-reference/usage",
              "api-reference/health"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/elementnl/StashFyle",
          "icon": "github"
        }
      ],
      "anchorsPosition": "bottom"
    }
  },
  "logo": {
    "light": "/images/logo-light.svg",
    "dark": "/images/logo-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://stashfyle.com/dashboard"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://stashfyle.com/api-keys"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/elementnl/StashFyle"
    }
  },
  "api": {
    "baseUrl": "https://api.stashfyle.com",
    "auth": {
      "method": "bearer",
      "name": "Authorization",
      "inputPrefix": "Bearer "
    },
    "playground": {
      "mode": "simple"
    }
  }
}
